<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.htmlCell{
			padding-top:7px;
			padding-bottom:7px;
		}
		
	</ui:style>
	<g:VerticalPanel spacing="4">
		<g:HTML ui:field="_text" styleName="{style.htmlCell}"></g:HTML>				
		<g:HorizontalPanel>
			<g:cell width="100%">
				<g:Button ui:field="_okButton">OK</g:Button>
			</g:cell>			
			<g:Button ui:field="_cancelButton">Cancel</g:Button>
		</g:HorizontalPanel>
	</g:VerticalPanel>
</ui:UiBinder> 